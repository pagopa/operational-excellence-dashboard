{
  "position": {
    "x": ${x},
    "y": ${y},
    "colSpan": ${colspan},
    "rowSpan": ${rowspan}
  },
  "metadata": {
    "inputs": [{
        "name": "resourceTypeMode",
        "isOptional": true
      },
      {
        "name": "ComponentId",
        "isOptional": true
      },
      {
        "name": "Scope",
        "value": {
          "resourceIds": [ ${scopes} ]
        },
        "isOptional": true
      },
      {
        "name": "PartId",
        "isOptional": true
      },
      {
        "name": "Version",
        "value": "2.0",
        "isOptional": true
      },
      {
        "name": "TimeRange",
        "value": "${range}",
        "isOptional": true
      },
      {
        "name": "DashboardId",
        "isOptional": true
      },
      {
        "name": "DraftRequestParameters",
        "value": {
          "scope": "hierarchy"
        },
        "isOptional": true
      },
      {
        "name": "Query",
        "value": "${query}",
        "isOptional": true
      },
      {
        "name": "ControlType",
        "value": "FrameControlChart",
        "isOptional": true
      },
      {
        "name": "SpecificChart",
        "value": "Line",
        "isOptional": true
      },
      {
        "name": "PartTitle",
        "value": "Analytics",
        "isOptional": true
      },
      {
        "name": "PartSubTitle",
        "value": "${subtitle}",
        "isOptional": true
      },
      {
        "name": "Dimensions",
        "value": {
          "xAxis": {
            "name": "${x_label}",
            "type": "datetime"
          },
          "yAxis": [{
              "name": "${y_label}",
              "type": "real"
            },
            {
              "name": "watermark",
              "type": "real"
            }
          ],
          "splitBy": [ ${split_by} ],
          "aggregation": "${aggregation}"
        },
        "isOptional": true
      },
      {
        "name": "LegendOptions",
        "value": {
          "isEnabled": true,
          "position": "Bottom"
        },
        "isOptional": true
      },
      {
        "name": "IsQueryContainTimeRange",
        "value": false,
        "isOptional": true
      }
    ],
    "type": "Extension/Microsoft_OperationsManagementSuite_Workspace/PartType/LogsDashboardPart",
    "settings": {
      "content": {
        "Query": "${query}",
        "PartTitle": "${title}"
      }
    }
  }
}
